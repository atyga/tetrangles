    <link href="https://fonts.googleapis.com/css2?family=Nunito&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" type="text/css" href="~/icons/font/flaticon.css" />

    <style>



        a:active {
            color: red !important;
        }

        a:hover {
            color: green;
        }

        .overlay {
            position: absolute;
            bottom: 100%;
            left: 0;
            right: 0;
            background-color: #fff;
            overflow: hidden;
            width: 100%;
            height: 0;
            transition: .5s ease;
            opacity: 0.8;
        }

        input, textarea {
            background-color: rgba(247, 247, 247, 1) !important;
        }

        form > .btn {
        }

        .house:hover .overlay {
            bottom: 0;
            height: 100%;
        }

        .rent:hover .overlay {
            bottom: 0;
            height: 100%;
        }

        .carport:hover .overlay {
            bottom: 0;
            height: 100%;
        }

        .steel:hover .overlay {
            bottom: 0;
            height: 100%;
        }

        .text {
            color: black;
            font-size: 20px;
            position: absolute;
            top: 50%;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
            text-align: center;
        }

        .navbar-brand img {
            width: 150px;
            height: 80px;
        }

        .navbar-toggler-icon {
            color: #fff;
        }
    </style>
</head>
